{
  "scopeName": "source.prox",
  "patterns": [
    {
      "match": "\b(if|else|while|for|def|return)\b",
      "name": "keyword.control.prox"
    },
    {
      "match": "\b(True|False|None)\b",
      "name": "constant.language.prox"
    },
    {
      "match": "\b[0-9]+\b",
      "name": "constant.numeric.prox"
    },
    {
      "match": "\".*?\"",
      "name": "string.quoted.double.prox"
    }
  ]
}